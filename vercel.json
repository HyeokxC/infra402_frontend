{
    "buildCommand": "cd frontend && npm install --legacy-peer-deps && npm run build",
    "outputDirectory": "frontend/dist",
    "framework": null,
    "headers": [
        {
            "source": "/.well-known/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}